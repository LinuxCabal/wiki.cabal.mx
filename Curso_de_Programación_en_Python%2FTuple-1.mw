== Tuple-1.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

# Un tuple vacio
a = ()
# Un tuple de solo uno elemento
b = ( "one", )
# Un tuple de dos elementos
c = ( "one", "two" )

print( "\na es", a )
print( "Largo de a es", len( a ) )

print( "\nb es", b )
print( "Largo de b es", len ( b ) )

print( "\nc es", c )
print( "Largo de c es", len( c ) )

</syntaxhighlight>

