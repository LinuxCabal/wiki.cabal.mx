== Prog15 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

/* Prog15.c -- Ancho de campo */

#include <stdio.h>

#define PAGINAS 931

int main( void )
{
  printf( "*%d*\n", PAGINAS );
  printf( "*%2d*\n", PAGINAS );
  printf( "*%10d*\n", PAGINAS );
  printf( "*123456789012345*\n" );
  printf( "*%-10d*\n", PAGINAS );

  return 0;
}

