== Prog9 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

/*  Prog9.c -- floats, doubles y  print() */

#include <stdio.h>

int main( void )
{
  float Float = 32000.0;
  double Double = 2.14e9;
  long double LongDouble = 5.32e-5;

  printf( "%f puede escribirse cómo %e\n", Float, Float );
  printf( "%f puede escribirse cómo %e\n", Double, Double );
  printf( "%Lf puede escribirse cómo %Le\n", LongDouble, LongDouble );

  return 0;
}

</syntaxhighlight>

