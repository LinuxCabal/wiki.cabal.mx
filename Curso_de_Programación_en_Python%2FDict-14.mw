#!/usr/bin/python3
#-*-coding: utf-8 -*-

# Crear una list de pares de tuples.
Pares = [ ("Gato", "Miau" ), ( "Perro", "Wooof" ), ( "Ave", "Twit" ) ]

# Convertir la lista en dictionary.
DictNuevo = dict( Pares )

print( "DictNuevo:", DictNuevo )
print( "DictNuevo.get( \"Perro\" ):", DictNuevo.get( "Perro" ) )
print( "len( DictNuevo ):", len( DictNuevo ) )

