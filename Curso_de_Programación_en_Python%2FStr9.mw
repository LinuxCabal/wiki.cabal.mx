#!/usr/bin/python3
#-*-coding: utf-8 -*-

Frase = "Bienvenido a nuestro curso de Python"

print( "\nEn la frase:\n\"", Frase, "\"\n ", sep='' )
print( "Usamos .startswith a ver si la frase se inicio con la letra \'B\'\n",
        Frase.startswith( 'B' ) )
print( "Usamos .startswith a ver si la frase se inicio con la letra \'b\'\n",
        Frase.startswith( 'b' ) )
print( "Usamos .startswith a ver si la frase se inicio con la letra \'n\'\n",
        Frase.startswith( 'n' ) )
print( "Usamos .startswith a ver si la frase se inicio con las letras \'Bi\'\n",
        Frase.startswith( 'Bi' ) )
print( "Usamos .endswith a ver si la frase se termino con la letra \'n\'\n",
        Frase.endswith( 'n' ) )
print( "Usamos .endswith a ver si la frase se termino con las letras \'hon\'\n",
        Frase.endswith( 'hon' ) )

