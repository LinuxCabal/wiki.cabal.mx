== Tuple-5.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Amigos = ( "Rebecca", "Julio", "LuLu", "Annabelle" )
print( "\nEl tuple Amigos es:\n{}" .format( Amigos ) )

print( "\nUsando max() a calcular el ultimo nombre por orden alfabético:\n{}"
          .format( max( Amigos ) ) )

print( "\nUsando min() a calcular el primer nombre por orden alfabético:\n{}"
          .format( min( Amigos ) ) )

Ganancias = ( 1000, 2000, 500, 4000 )

print( "\nEl tuple Ganancias es:\n{}" .format( Ganancias ) )

print( "\nUsando max() a calcular la mayor cantidad:\n{}"
          .format( max( Ganancias ) ) )

print( "\nUsando min() a calcular la menor cantidad:\n{}"
          .format( min( Ganancias ) ) )

</syntaxhighlight>

