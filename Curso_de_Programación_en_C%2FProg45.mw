== Prog45 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

int main( void )
{
  const int NUMERO = 22;
  int count;

  for( count = 1; count <= NUMERO; count++ )
    printf( "El valor de count es: %i\n", count );

  return 0;
}

