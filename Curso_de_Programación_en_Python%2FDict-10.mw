<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

OS = {"Mac OS X" : 1, "MS Windows" : 5, "GNU & linux" : 1, "Android" : 10 }

# Display dictionary.
print( "Antes de DEL:", OS )

# Remove key-value at "MS Windows" key.
del OS["MS Windows"]

# Display dictionary.
print( "Después de DEL:", OS )

