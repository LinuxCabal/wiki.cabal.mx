#!/usr/bin/python3
#-*-coding: utf-8 -*-

# Para imprimir un Diccionario con items

tabla = { 'Julio': 4127, 'Mario': 4098, 'José': 7678 }

print( "{0:10}     {1:^16}" .format( "Nombre", "Extensión" ) )

for Nombre, Extensión in tabla.items():
  print( '{0:10} ==> {1:10d}' .format( Nombre, Extensión ) )

