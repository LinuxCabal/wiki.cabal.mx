var = 100

if var < 200:
  print( "var es menor que 200" )
  if var == 150:
    print( "var vale 150" )
  elif var == 100:
    print( "var vale 100" )
  elif var == 50:
    print( "var vale 50" )
elif var < 50:
  print( "var es menor que 50" )
else:
  print( "No puedo determinar cual es el valor de var" )

