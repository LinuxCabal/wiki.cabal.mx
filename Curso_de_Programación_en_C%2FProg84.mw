== Prog84 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

#include <stdio.h>

#define DEF "Soy una cadena #define."

int main( void )
{
  char         str1[80] = "Un arreglo se inicializó en mí.";
  const char * str2     = "Un pointer se inicializó en mí.";

  puts( "Soy un argumento a puts()." );

  puts(DEF);

  puts(str1);

  puts(str2);

  puts(&str1[5]);

  puts(str2+4);

  return 0;
}

</syntaxhighlight>

