>>> import re
>>>
>>> MatchObject2 = re.search("([0-9]+).*: (.*)", "Número de Cliente: 234321, Fecha: 26 Noviembre, 2015" )
>>> 
>>> MatchObject2.group()
'234321, Fecha: 26 Noviembre, 2015'
>>> 
>>> MatchObject2.group(1)
'234321'
>>> 
>>> MatchObject2.group(2)
'26 Noviembre, 2015'
>>> 
