Uno = 1
Dos = 2
Tres = 3

Valores = ( 33, "Leo", -66.77, ( Uno, Dos, Tres ) )
print( "\nEl tuple Valores es:\n{}" .format( Valores ) )

print( "\nPuedes duplicar Valores con Valores[:]\n{}" .format( Valores[:] ) )

print( "\nPuedes adquirir todos los Valores de posici贸n 2 adelante con Valores[2:]\n{}"
            .format( Valores[2:] ) )

print( "\nPuedes adquir el primer Valor con Valores[:1]\n{}"
            .format( Valores[:1] ) )

print( "\nPuedes adquir Valores en posiciones 1, 2 y 3 con Valores[1:4]\n{}"
            .format( Valores[1:4] ) )

