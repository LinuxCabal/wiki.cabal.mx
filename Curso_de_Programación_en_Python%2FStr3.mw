== Str3.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Frase = "Hola Mundo"
i = 0

while i < len( Frase ):
  print( "La letra \'", Frase[i], "\' aparece ", Frase.count( Frase[i] ),
         " veces en la frase: ",  Frase, sep='' )
  i += 1
print( "\nListo\n" )

