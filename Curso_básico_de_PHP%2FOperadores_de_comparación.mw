      $Examen = 4;

      print( "<p>
                |"  . ( $Examen == 4 )    .
               "|<br />
                |"  . ( $Examen == '4' )  .
               "|<br />
                |"  . ( $Examen === '4' ) .
               "|<br />
                |"  . ( $Examen != 4 )    .
               "|<br />
                |"  . ( $Examen != '4' )  .
               "|<br />
                |"  . ( $Examen !== 4 )    .
               "|<br />
                |"  . ( $Examen !== '4' )  .
               "|<br />
                |"  . ( $Examen > 4 )     .
               "|<br />
                |"  . ( $Examen > '4' )   .
               "|<br />
                |"  . ( $Examen < 4 )     .
               "|<br />
                |"  . ( $Examen < '4' )   .
               "|<br />
                |"  . ( $Examen >= 4 )    .
               "|<br />
                |"  . ( $Examen >= '4' )  .
               "|<br />
                |"  . ( $Examen <= 4 )    .
               "|<br />
                |"  . ( $Examen <= '4' )  .
               "|
              </p>
