#!/usr/bin/python3
#-*-coding: utf-8 -*-

Gastos = { "Departamento" : 3500, "Coche" : 750, "Utilidades" : 4500 }

# Convert to list of tuples
GastosItems = Gastos.items()

print( "El tuple GastosItems:", GastosItems, "\n" )
# Loop and display tuple items
for GastosItem in GastosItems:
    print( "Gasto:", GastosItem[0] )
    print( "Costo:", GastosItem[1] )
    print( "" )

