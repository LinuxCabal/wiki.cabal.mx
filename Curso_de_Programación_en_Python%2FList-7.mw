== List-7.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Nombres = [ "Luis", "Hortensia", "Luela", "José", "Jorge" ]

print( "\nLa list Nombres, ahorra es:\n{}" .format( Nombres ) )

# Remueve este valor
Nombres.remove( "Luela" )
print( "\nLa list Nombres, después de Nombres.remove( \"Luela\" ) es:\n{}"
           .format( Nombres ) )

# Remueve todos después de los primer dos nombres
del Nombres[3:]
print( "\nLa list Nombres, después de del Nombres[3:] es:\n{}" 
           .format( Nombres ) )

# Remueve todos hasta el último nombre
del Nombres[:-1]
print( "\nLa list Nombres, después de del Nombres[:-1] es:\n{}" 
           .format( Nombres ) )

</syntaxhighlight>

