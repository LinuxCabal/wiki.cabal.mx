#!/usr/bin/python3
#-*-coding: utf-8 -*-


Set1 = { 1, "Gato", 3 }
Set2 = { 6, "Perro", "Gato", 4, 3 }

print( "Set1:", Set1 )
print( "Set2:", Set2 )

# Union the sets.
Set3 = Set1.union( Set2 )
print( "Set3:", Set3 )

