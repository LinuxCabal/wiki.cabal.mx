== Prog59 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

#include <stdio.h>
#define MESES 12

int main( void )
{
  int dias[MESES] = {31,28,31,30,31,30,31,31,30,31,30,31};
  int index;

  for( index = 0; index < MESES; index++ )
    printf( "Mes %d tiene %2d días.\n", index + 1, dias[index]);

  return 0;
}

