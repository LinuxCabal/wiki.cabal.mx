      print( "<p>
                Operador de DivisiónIgual
              </p>" );
      $x = 4;
      print( "<p>" . $x . "<br />" );
      $x = $x / 4;
      print(    $x . "<br />" );

      $x = 4;
      print(    $x . "<br />" );
      $x /= 4;
      print(    $x . "</p>");
