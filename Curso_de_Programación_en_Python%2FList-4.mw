== List-4.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Amigos = [ "Rebecca", "Julio", "Lulu", "Annabelle" ]
print( "\nLa list Amigos es:\n{}" .format( Amigos ) )

if "Lulu" in Amigos:
  print( "Descubrimos que Lulu es en la list Amigos usando \"in\"" )

if "Hortencia" not in Amigos:
  print( "Descubrimos que Hortencia no es en la list Amigos usando \"not in\"" )

</syntaxhighlight>

