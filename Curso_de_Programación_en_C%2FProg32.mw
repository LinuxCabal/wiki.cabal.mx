== Prog32 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

#include <stdio.h>

int main( void )
{

  printf( "Operador de Menos Igual\n" );

  int x = 4;
  printf ( "x = %i\n", x );

  x = x - 4;
  printf ( "x = %i\n", x );
 

  x = 4;
  printf ( "x = %i\n", x );

  x -= 4;
  printf ( "x = %i\n", x );

  printf( "Listo.\n" );

  return 0;
}
</syntaxhighlight>

