== Tuple-2.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

print( "\nTuples son inmutables\n" )

MiTup = ( "Azul", "Verde", "Gris" )

for i in range( len( MiTup ) ):
  print( "MiTup[{}] es {}" .format( i, MiTup[i] ) )
print( "A Decir ahorra que MiTup[1] = \"Amarillo\" nos da:\n" )

MiTup[1] = "Amarillo"

</syntaxhighlight>

