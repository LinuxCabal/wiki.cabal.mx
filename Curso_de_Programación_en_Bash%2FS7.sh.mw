X=-3

[ $X -le 0 ] &&  echo "\$X = $X que es menor que o igual a cero"

[ "$X" -ge "0" ] &&                 \
  echo "\$X = $X que es mayor que o igual a cero"

X=0
[ "$X" = "0" ] && echo "\$X = $X que es la cadena \"0\""

X=Hola
