== Tuple-4.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Medidas1  = ( 7, 11, -13 )
Medidas2 = ( 22, -33, "N/A", -22.56 )

print( "\nMedidas1 son {}" .format( Medidas1 ) )
print( "Medidas2 son {}\n" .format( Medidas2 ) )

print( "Agregamos Medidas2 a Medidas1:\n", Medidas1 + Medidas2, "\n" )

print( "Multiplicamos Medidas2 por 3:\n", Medidas2 * 3, "\n" )

</syntaxhighlight>

