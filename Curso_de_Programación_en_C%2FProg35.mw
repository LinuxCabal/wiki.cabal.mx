== Prog35 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

#include <stdio.h>

int main( void )
{

  printf( "Operador de Modulo Igual\n" );

  int x = 11;
  printf ( "x = %i\n", x );

  x = x % 3;
  printf ( "x = %i\n", x );
 

  x = 11;
  printf ( "x = %i\n", x );

  x %= 3;
  printf ( "x = %i\n", x );

  printf( "Listo.\n" );

  return 0;
}
</syntaxhighlight>

