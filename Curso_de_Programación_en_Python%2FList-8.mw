== List-8.py ==

<syntaxhighlight lang="python" line="GESHI_FANCY_LINE_NUMBERS">

#!/usr/bin/python3
#-*-coding: utf-8 -*-

Cosas = [ "Libro", "Plato", "Plato", "Libro", "Tasa", "Libro" ]

print( "La list Cosas es:\n{}" .format( Cosas ) )

print( "Usando count() vemos que tenemos {0} Libro(s), {1} Plato(s) y {2} Tasa(s)"
          .format( Cosas.count("Libro"), Cosas.count("Plato"), Cosas.count("Tasa") ) )

</syntaxhighlight>

