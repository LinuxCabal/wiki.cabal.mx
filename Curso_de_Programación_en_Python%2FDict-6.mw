#!/usr/bin/python3
#-*-coding: utf-8 -*-

Accesos = { "Página principal" : 125, "Mapa de sitio" : 27, "Acerca de" : 43 }
# Sort the keys from the dictionary.
Llaves = sorted( Accesos.keys() )

print( "La lista de Llaves en el Dictionary :", Accesos.keys() )
print( "La lista de Llaves, ordenada, en el Dictionary :", Llaves )

print( "\nMira cómo la lista de llaves asignadas es diferente de la lista de llaves devueltas" )

