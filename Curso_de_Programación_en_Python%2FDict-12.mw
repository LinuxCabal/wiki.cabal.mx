#!/usr/bin/python3
#-*-coding: utf-8 -*-

DictOrig = { "Caja" : 12, "Penguino" : 2, "Manzana" : 15}

DictCopy = DictOrig.copy()

DictCopy["Caja"] += 200
DictCopy["Manzana"] -= 9

# Original is still the same
print( "DictOrig:", DictOrig )
print( "DictCopy[", DictCopy )

