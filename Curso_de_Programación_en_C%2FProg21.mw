== Prog21 ==

<syntaxhighlight lang="c" line="GESHI_FANCY_LINE_NUMBERS">

/* Prog21.c -- Valor regresado por printf() */

#include <stdio.h>

int main( void )
{
  int EbullicionH2O = 100,
      ValorRegresado;

  ValorRegresado = printf( "%dC es el punto de ebullicion del agua.\n", EbullicionH2O );
  printf( "12345678901234567890123456789012345\n" );
  printf( "La funcion printf() escribio %d caracteres.\n", ValorRegresado );

  return 0;
}

</syntaxhighlight>

