#!/usr/bin/python3
#-*-coding: utf-8 -*-

Capital_Pais={ "United States" : "Washington", 
               "US" : "Washington", 
               "Canada" : "Ottawa",
               "Germany": "Berlin",
               "France" : "Paris",
               "England" : "London",
               "UK" : "London",
               "Switzerland" : "Bern",
               "Austria" : "Vienna",
               "Netherlands" : "Amsterdam" }

print( "\nPaises y sus capitales:")
for c in Capital_Pais:
    print("{Pais:^14s}: {Capital}" .format( Pais=c, Capital=Capital_Pais[c] ) )

