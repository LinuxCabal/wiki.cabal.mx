/* Prog28.c -- El operador de asignación */

#include <stdio.h>

int main( void )
{
  int Juan,
      Carla,
      Tomas;

  Juan = Carla = Tomas = 68;

  printf( "                  Juan   Carla    Tomas\n" );
  printf( "Resultados de la competición: %4d %8d %8d\n", Juan, Carla, Tomas );

  return 0;
}

